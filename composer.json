{
    "name": "bobfridley/laravel-freshdesk",
    "description": "A Freshdesk bridge for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "freshdesk",
        "Bob Fridley",
        "BobFridley"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Fridley",
            "email": "bobfridley@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/contracts": "^5.2.8",
        "illuminate/support": "^5.2.8",
        "graham-campbell/manager": "^3.3.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "BobFridley\\Freshdesk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BobFridley\\Tests\\Freshdesk\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
